<script>
  (function() {
    class FooterBlock{{ai_gen_id}} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.setupFormValidation();
      }

      setupFormValidation() {
        const form = this.querySelector('.ai-footer-signup-form-{{ ai_gen_id }}');
        if (!form) return;

        const emailInput = form.querySelector('.ai-footer-email-input-{{ ai_gen_id }}');
        
        if (emailInput) {
          emailInput.addEventListener('invalid', function(e) {
            e.preventDefault();
            emailInput.setCustomValidity('Please enter a valid email address');
          });

          emailInput.addEventListener('input', function() {
            emailInput.setCustomValidity('');
          });
        }
      }
    }

    customElements.define('footer-block-{{ ai_gen_id }}', FooterBlock{{ai_gen_id}});
  })();
</script>

