{% doc %}
  @prompt
    Create a footer section matching this exact layout: 
    - Left side: "LATITUDE" brand name in bold uppercase
    - Four columns: "LINKS" (Home, About Us, FAQ, Contact), "LEGAL" (Privacy Policy, Refund Policy, Shipping Policy, Terms of Service), "THE BRAND" (brand description text)
    - Below: Email signup form with "SIGN UP FOR 15% OFF" heading, email input field with subscribe button
    - Social media icons row (Twitter, Facebook, Pinterest, Instagram, TikTok, Snapchat, YouTube) aligned to the right
    - Payment method icons at bottom center (Visa, Mastercard, Amex, PayPal, Diners, Discover)
    - Copyright text at bottom
    - Clean minimalist design with proper spacing, fully responsive with mobile stacking
    - Use Flexbox/Grid layout, include hover effects on links
    - Match the typography, spacing and alignment from the reference image provided
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-footer-{{ai_gen_id}} {
    display: block;
    width: 100%;
    background-color: {{ block.settings.background_color }};
    color: {{ block.settings.text_color }};
    padding: {{ block.settings.padding_top }}px 20px {{ block.settings.padding_bottom }}px;
  }

  .ai-footer-container-{{ai_gen_id}} {
    max-width: 1400px;
    margin: 0 auto;
  }

  .ai-footer-brand-{{ai_gen_id}} {
    font-size: {{ block.settings.brand_font_size }}px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 2px;
    margin-bottom: 40px;
    color: {{ block.settings.text_color }};
  }

  .ai-footer-columns-{{ai_gen_id}} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 40px;
    margin-bottom: 50px;
  }

  .ai-footer-column-{{ai_gen_id}} {
    display: flex;
    flex-direction: column;
  }

  .ai-footer-column-title-{{ai_gen_id}} {
    font-size: {{ block.settings.column_title_size }}px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1px;
    margin-bottom: 20px;
    color: {{ block.settings.text_color }};
  }

  .ai-footer-link-{{ai_gen_id}} {
    color: {{ block.settings.link_color }};
    text-decoration: none;
    font-size: {{ block.settings.link_font_size }}px;
    line-height: 2;
    transition: color 0.3s ease, opacity 0.3s ease;
  }

  .ai-footer-link-{{ai_gen_id}}:hover {
    color: {{ block.settings.link_hover_color }};
    opacity: 0.8;
  }

  .ai-footer-description-{{ai_gen_id}} {
    font-size: {{ block.settings.description_font_size }}px;
    line-height: 1.6;
    color: {{ block.settings.text_color }};
  }

  .ai-footer-signup-{{ai_gen_id}} {
    margin-bottom: 40px;
  }

  .ai-footer-signup-title-{{ai_gen_id}} {
    font-size: {{ block.settings.signup_title_size }}px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1px;
    margin-bottom: 20px;
    color: {{ block.settings.text_color }};
  }

  .ai-footer-signup-form-{{ai_gen_id}} {
    display: flex;
    gap: 10px;
    max-width: 500px;
  }

  .ai-footer-email-input-{{ai_gen_id}} {
    flex: 1;
    padding: 12px 16px;
    border: 1px solid {{ block.settings.input_border_color }};
    background-color: {{ block.settings.input_background }};
    color: {{ block.settings.input_text_color }};
    font-size: 14px;
    border-radius: {{ block.settings.input_border_radius }}px;
  }

  .ai-footer-email-input-{{ai_gen_id}}::placeholder {
    color: {{ block.settings.input_placeholder_color }};
  }

  .ai-footer-subscribe-btn-{{ai_gen_id}} {
    padding: 12px 24px;
    background-color: {{ block.settings.button_background }};
    color: {{ block.settings.button_text_color }};
    border: none;
    font-size: 14px;
    font-weight: 600;
    text-transform: uppercase;
    cursor: pointer;
    border-radius: {{ block.settings.button_border_radius }}px;
    transition: background-color 0.3s ease;
  }

  .ai-footer-subscribe-btn-{{ai_gen_id}}:hover {
    background-color: {{ block.settings.button_hover_background }};
  }

  .ai-footer-social-row-{{ai_gen_id}} {
    display: flex;
    justify-content: flex-end;
    gap: 20px;
    margin-bottom: 40px;
    flex-wrap: wrap;
  }

  .ai-footer-social-link-{{ai_gen_id}} {
    color: {{ block.settings.social_icon_color }};
    transition: color 0.3s ease, transform 0.3s ease;
    display: inline-flex;
  }

  .ai-footer-social-link-{{ai_gen_id}}:hover {
    color: {{ block.settings.social_icon_hover_color }};
    transform: translateY(-2px);
  }

  .ai-footer-social-link-{{ai_gen_id}} svg {
    width: {{ block.settings.social_icon_size }}px;
    height: {{ block.settings.social_icon_size }}px;
  }

  .ai-footer-payment-{{ai_gen_id}} {
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 15px;
    margin-bottom: 30px;
    flex-wrap: wrap;
  }

  .ai-footer-payment-icon-{{ai_gen_id}} {
    width: {{ block.settings.payment_icon_width }}px;
    height: auto;
    opacity: 0.7;
    transition: opacity 0.3s ease;
  }

  .ai-footer-payment-icon-{{ai_gen_id}}:hover {
    opacity: 1;
  }

  .ai-footer-copyright-{{ai_gen_id}} {
    text-align: center;
    font-size: {{ block.settings.copyright_font_size }}px;
    color: {{ block.settings.copyright_color }};
    padding-top: 20px;
    border-top: 1px solid {{ block.settings.divider_color }};
  }

  .ai-footer-success-{{ai_gen_id}} {
    color: {{ block.settings.success_color }};
    margin-top: 10px;
    font-size: 14px;
  }

  .ai-footer-error-{{ai_gen_id}} {
    color: {{ block.settings.error_color }};
    margin-top: 10px;
    font-size: 14px;
  }

  @media screen and (max-width: 768px) {
    .ai-footer-columns-{{ai_gen_id}} {
      grid-template-columns: 1fr;
      gap: 30px;
    }

    .ai-footer-social-row-{{ai_gen_id}} {
      justify-content: center;
    }

    .ai-footer-signup-form-{{ai_gen_id}} {
      flex-direction: column;
    }

    .ai-footer-brand-{{ai_gen_id}} {
      text-align: center;
    }

    .ai-footer-signup-title-{{ai_gen_id}} {
      text-align: center;
    }
  }
{% endstyle %}

<footer-block-{{ai_gen_id}} class="ai-footer-{{ai_gen_id}}" {{ block.shopify_attributes }}>
  <div class="ai-footer-container-{{ai_gen_id}}">
    {% if block.settings.brand_name != blank %}
      <div class="ai-footer-brand-{{ai_gen_id}}">{{ block.settings.brand_name }}</div>
    {% endif %}

    <div class="ai-footer-columns-{{ai_gen_id}}">
      <div class="ai-footer-column-{{ai_gen_id}}">
        <h3 class="ai-footer-column-title-{{ai_gen_id}}">{{ block.settings.column_1_title }}</h3>
        {% if block.settings.column_1_link_1_text != blank %}
          <a href="{{ block.settings.column_1_link_1_url }}" class="ai-footer-link-{{ai_gen_id}}">{{ block.settings.column_1_link_1_text }}</a>
        {% endif %}
        {% if block.settings.column_1_link_2_text != blank %}
          <a href="{{ block.settings.column_1_link_2_url }}" class="ai-footer-link-{{ai_gen_id}}">{{ block.settings.column_1_link_2_text }}</a>
        {% endif %}
        {% if block.settings.column_1_link_3_text != blank %}
          <a href="{{ block.settings.column_1_link_3_url }}" class="ai-footer-link-{{ai_gen_id}}">{{ block.settings.column_1_link_3_text }}</a>
        {% endif %}
        {% if block.settings.column_1_link_4_text != blank %}
          <a href="{{ block.settings.column_1_link_4_url }}" class="ai-footer-link-{{ai_gen_id}}">{{ block.settings.column_1_link_4_text }}</a>
        {% endif %}
      </div>

      <div class="ai-footer-column-{{ai_gen_id}}">
        <h3 class="ai-footer-column-title-{{ai_gen_id}}">{{ block.settings.column_2_title }}</h3>
        {% if block.settings.column_2_link_1_text != blank %}
          <a href="{{ block.settings.column_2_link_1_url }}" class="ai-footer-link-{{ai_gen_id}}">{{ block.settings.column_2_link_1_text }}</a>
        {% endif %}
        {% if block.settings.column_2_link_2_text != blank %}
          <a href="{{ block.settings.column_2_link_2_url }}" class="ai-footer-link-{{ai_gen_id}}">{{ block.settings.column_2_link_2_text }}</a>
        {% endif %}
        {% if block.settings.column_2_link_3_text != blank %}
          <a href="{{ block.settings.column_2_link_3_url }}" class="ai-footer-link-{{ai_gen_id}}">{{ block.settings.column_2_link_3_text }}</a>
        {% endif %}
        {% if block.settings.column_2_link_4_text != blank %}
          <a href="{{ block.settings.column_2_link_4_url }}" class="ai-footer-link-{{ai_gen_id}}">{{ block.settings.column_2_link_4_text }}</a>
        {% endif %}
      </div>

      <div class="ai-footer-column-{{ai_gen_id}}">
        <h3 class="ai-footer-column-title-{{ai_gen_id}}">{{ block.settings.column_3_title }}</h3>
        {% if block.settings.column_3_description != blank %}
          <div class="ai-footer-description-{{ai_gen_id}}">{{ block.settings.column_3_description }}</div>
        {% endif %}
      </div>
    </div>

    <div class="ai-footer-signup-{{ai_gen_id}}">
      {% if block.settings.signup_title != blank %}
        <h3 class="ai-footer-signup-title-{{ai_gen_id}}">{{ block.settings.signup_title }}</h3>
      {% endif %}
      {%- form 'customer', class: 'ai-footer-signup-form-' | append: ai_gen_id -%}
        <input type="hidden" name="contact[tags]" value="newsletter">
        <input
          type="email"
          name="contact[email]"
          class="ai-footer-email-input-{{ai_gen_id}}"
          placeholder="{{ block.settings.email_placeholder }}"
          aria-label="Email address"
          required
        >
        <button type="submit" class="ai-footer-subscribe-btn-{{ai_gen_id}}">
          {{ block.settings.button_text }}
        </button>
      {%- endform -%}
      {%- if form.posted_successfully? -%}
        <div class="ai-footer-success-{{ai_gen_id}}">{{ block.settings.success_message }}</div>
      {%- endif -%}
      {%- if form.errors -%}
        <div class="ai-footer-error-{{ai_gen_id}}">{{ form.errors.messages.email }}</div>
      {%- endif -%}
    </div>

