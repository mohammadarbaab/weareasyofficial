    <div class="ai-footer-main-{{ ai_gen_id }}">
      <div class="ai-footer-column-{{ ai_gen_id }}">
        <h3 class="ai-footer-column-title-{{ ai_gen_id }}">{{ block.settings.links_heading }}</h3>
        {% if block.settings.link_1_text != blank %}
          <a href="{{ block.settings.link_1_url }}" class="ai-footer-link-{{ ai_gen_id }}">
            {{ block.settings.link_1_text }}
          </a>
        {% endif %}
        {% if block.settings.link_2_text != blank %}
          <a href="{{ block.settings.link_2_url }}" class="ai-footer-link-{{ ai_gen_id }}">
            {{ block.settings.link_2_text }}
          </a>
        {% endif %}
        {% if block.settings.link_3_text != blank %}
          <a href="{{ block.settings.link_3_url }}" class="ai-footer-link-{{ ai_gen_id }}">
            {{ block.settings.link_3_text }}
          </a>
        {% endif %}
        {% if block.settings.link_4_text != blank %}
          <a href="{{ block.settings.link_4_url }}" class="ai-footer-link-{{ ai_gen_id }}">
            {{ block.settings.link_4_text }}
          </a>
        {% endif %}
      </div>

      <div class="ai-footer-column-{{ ai_gen_id }}">
        <h3 class="ai-footer-column-title-{{ ai_gen_id }}">{{ block.settings.legal_heading }}</h3>
        {% if block.settings.legal_1_text != blank %}
          <a href="{{ block.settings.legal_1_url }}" class="ai-footer-link-{{ ai_gen_id }}">
            {{ block.settings.legal_1_text }}
          </a>
        {% endif %}
        {% if block.settings.legal_2_text != blank %}
          <a href="{{ block.settings.legal_2_url }}" class="ai-footer-link-{{ ai_gen_id }}">
            {{ block.settings.legal_2_text }}
          </a>
        {% endif %}
        {% if block.settings.legal_3_text != blank %}
          <a href="{{ block.settings.legal_3_url }}" class="ai-footer-link-{{ ai_gen_id }}">
            {{ block.settings.legal_3_text }}
          </a>
        {% endif %}
        {% if block.settings.legal_4_text != blank %}
          <a href="{{ block.settings.legal_4_url }}" class="ai-footer-link-{{ ai_gen_id }}">
            {{ block.settings.legal_4_text }}
          </a>
        {% endif %}
      </div>

      <div class="ai-footer-column-{{ ai_gen_id }}">
        <h3 class="ai-footer-column-title-{{ ai_gen_id }}">{{ block.settings.brand_heading }}</h3>
        <div class="ai-footer-text-{{ ai_gen_id }}">
          {{ block.settings.brand_description }}
        </div>
      </div>

      <div class="ai-footer-column-{{ ai_gen_id }}">
        <div class="ai-footer-newsletter-{{ ai_gen_id }}">
          <h3 class="ai-footer-column-title-{{ ai_gen_id }}">{{ block.settings.newsletter_heading }}</h3>
          {%- form 'customer', id: 'ai-footer-newsletter-form-' | append: ai_gen_id -%}
            <input type="hidden" name="contact[tags]" value="newsletter">
            <div class="ai-footer-newsletter-form-{{ ai_gen_id }}">
              <input
                type="email"
                name="contact[email]"
                class="ai-footer-newsletter-input-{{ ai_gen_id }}"
                value="{{ form.email }}"
                aria-required="true"
                autocorrect="off"
                autocapitalize="off"
                autocomplete="email"
                {% if form.errors %}
                  aria-invalid="true"
                  aria-describedby="ai-footer-newsletter-error-{{ ai_gen_id }}"
                {% endif %}
                {% if form.posted_successfully? %}
                  aria-describedby="ai-footer-newsletter-success-{{ ai_gen_id }}"
                {% endif %}
                placeholder="{{ block.settings.newsletter_placeholder }}"
                required
              >
              <button type="submit" class="ai-footer-newsletter-button-{{ ai_gen_id }}">
                {{ block.settings.newsletter_button_text }}
              </button>
            </div>
            {%- if form.errors -%}
              <div
                id="ai-footer-newsletter-error-{{ ai_gen_id }}"
                class="ai-footer-newsletter-error-{{ ai_gen_id }}"
              >
                {{ form.errors.messages.email }}
              </div>
            {%- endif -%}
            {%- if form.posted_successfully? -%}
              <div
                id="ai-footer-newsletter-success-{{ ai_gen_id }}"
                class="ai-footer-newsletter-success-{{ ai_gen_id }}"
              >
                {{ block.settings.newsletter_success_message }}
              </div>
            {%- endif -%}
          {%- endform -%}
        </div>
      </div>
    </div>

    <div class="ai-footer-bottom-{{ ai_gen_id }}">
      <div class="ai-footer-copyright-{{ ai_gen_id }}">
        {{ block.settings.copyright_text }}
      </div>

      {% if block.settings.show_payment_icons %}
        <div class="ai-footer-payment-{{ ai_gen_id }}">
          {% if block.settings.show_visa %}
            <svg class="ai-footer-payment-icon-{{ ai_gen_id }}" viewBox="0 0 38 24" xmlns="http://www.w3.org/2000/svg" role="img" aria-labelledby="pi-visa">
              <title id="pi-visa">Visa</title>
