{% doc %}
  @prompt
    Create a custom footer section matching this layout: Brand name "LATITUDE" on the left, followed by four columns - "LINKS" (Home, About Us, FAQ, Contact), "LEGAL" (Privacy Policy, Refund Policy, Shipping Policy, Terms of Service), "THE BRAND" (brand description text), and a newsletter signup section with "SIGN UP FOR 15% OFF" heading and email input field. Include social media icons (Twitter, Facebook, Pinterest, Instagram, TikTok, Snapchat, YouTube) in the top right. Add payment method badges (Visa, Mastercard, Amex, PayPal, Diners, Discover) and copyright text at the bottom. Use a clean, modern typography with proper spacing, responsive grid layout that stacks on mobile, and subtle hover effects on links. Ensure the footer has a light background with dark text for contrast.
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-footer-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    background-color: {{ block.settings.background_color }};
    color: {{ block.settings.text_color }};
    padding: {{ block.settings.padding_top }}px 0 {{ block.settings.padding_bottom }}px;
  }

  .ai-footer-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-footer-top-{{ ai_gen_id }} {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    margin-bottom: 40px;
    gap: 20px;
  }

  .ai-footer-social-{{ ai_gen_id }} {
    display: flex;
    gap: 16px;
    align-items: center;
  }

  .ai-footer-social-link-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
    transition: color 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-footer-social-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.link_hover_color }};
  }

  .ai-footer-social-link-{{ ai_gen_id }} svg {
    width: {{ block.settings.social_icon_size }}px;
    height: {{ block.settings.social_icon_size }}px;
  }

  .ai-footer-main-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 1fr 1fr 1fr 1.5fr;
    gap: 40px;
    margin-bottom: 40px;
  }

  .ai-footer-column-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 12px;
  }

  .ai-footer-column-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.heading_size }}px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    margin-bottom: 8px;
    color: {{ block.settings.heading_color }};
  }

  .ai-footer-brand-name-{{ ai_gen_id }} {
    font-size: {{ block.settings.brand_size }}px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1px;
    color: {{ block.settings.heading_color }};
  }

  .ai-footer-link-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
    text-decoration: none;
    font-size: {{ block.settings.link_size }}px;
    line-height: 1.6;
    transition: color 0.3s ease;
  }

  .ai-footer-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.link_hover_color }};
  }

  .ai-footer-text-{{ ai_gen_id }} {
    font-size: {{ block.settings.text_size }}px;
    line-height: 1.6;
    color: {{ block.settings.text_color }};
  }

  .ai-footer-newsletter-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 12px;
  }

  .ai-footer-newsletter-form-{{ ai_gen_id }} {
    display: flex;
    gap: 8px;
  }

  .ai-footer-newsletter-input-{{ ai_gen_id }} {
    flex: 1;
    padding: 12px 16px;
    border: 1px solid {{ block.settings.input_border_color }};
    border-radius: {{ block.settings.input_border_radius }}px;
    background-color: {{ block.settings.input_background }};
    color: {{ block.settings.text_color }};
    font-size: 14px;
  }

  .ai-footer-newsletter-input-{{ ai_gen_id }}::placeholder {
    color: {{ block.settings.text_color | color_modify: 'alpha', 0.6 }};
  }

  .ai-footer-newsletter-button-{{ ai_gen_id }} {
    padding: 12px 24px;
    background-color: {{ block.settings.button_background }};
    color: {{ block.settings.button_text_color }};
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: 14px;
    font-weight: 600;
    cursor: pointer;
    transition: background-color 0.3s ease;
    white-space: nowrap;
  }

  .ai-footer-newsletter-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_background }};
  }

  .ai-footer-newsletter-success-{{ ai_gen_id }} {
    color: {{ block.settings.success_color }};
    font-size: 14px;
    margin-top: 8px;
  }

  .ai-footer-newsletter-error-{{ ai_gen_id }} {
    color: {{ block.settings.error_color }};
    font-size: 14px;
    margin-top: 8px;
  }

  .ai-footer-bottom-{{ ai_gen_id }} {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-top: 30px;
    border-top: 1px solid {{ block.settings.border_color }};
    gap: 20px;
  }

  .ai-footer-copyright-{{ ai_gen_id }} {
    font-size: {{ block.settings.copyright_size }}px;
    color: {{ block.settings.text_color | color_modify: 'alpha', 0.7 }};
  }

  .ai-footer-payment-{{ ai_gen_id }} {
    display: flex;
    gap: 12px;
    align-items: center;
    flex-wrap: wrap;
  }

  .ai-footer-payment-icon-{{ ai_gen_id }} {
    width: {{ block.settings.payment_icon_width }}px;
    height: auto;
    opacity: 0.7;
    transition: opacity 0.3s ease;
  }

  .ai-footer-payment-icon-{{ ai_gen_id }}:hover {
    opacity: 1;
  }

  @media screen and (max-width: 990px) {
    .ai-footer-main-{{ ai_gen_id }} {
      grid-template-columns: 1fr 1fr;
      gap: 30px;
    }

    .ai-footer-top-{{ ai_gen_id }} {
      flex-direction: column;
      gap: 20px;
    }

    .ai-footer-social-{{ ai_gen_id }} {
      order: 2;
    }
  }

  @media screen and (max-width: 749px) {
    .ai-footer-main-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 30px;
    }

    .ai-footer-bottom-{{ ai_gen_id }} {
      flex-direction: column;
      align-items: flex-start;
      gap: 20px;
    }

    .ai-footer-newsletter-form-{{ ai_gen_id }} {
      flex-direction: column;
    }

    .ai-footer-newsletter-button-{{ ai_gen_id }} {
      width: 100%;
    }
  }
{% endstyle %}

